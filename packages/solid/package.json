{
  "name": "@motionone/solid",
  "version": "10.16.2",
  "description": "A tiny, performant animation library for SolidJS",
  "license": "MIT",
  "contributors": [
    "Damian Tarnawski <gthetarnav@gmail.com>",
    "David Di Biase <dave.dibiase@gmail.com>"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    "solid": "./dist/source/index.jsx",
    "import": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/size.js",
      "maxSize": "6.35 kB"
    }
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "yarn test:browser && yarn test:ssr",
    "test:browser": "jest --coverage --config jest.config.cjs",
    "test:ssr": "jest --config jest.ssrconfig.cjs",
    "measure": "bundlesize"
  },
  "dependencies": {
    "@motionone/dom": "^10.16.2",
    "@motionone/utils": "^10.15.1",
    "@solid-primitives/props": "^3.1.8",
    "@solid-primitives/refs": "^1.0.5",
    "@solid-primitives/transition-group": "^1.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "rollup-preset-solid": "^1.4.0",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.7.9",
    "solid-testing-library": "^0.5.1"
  },
  "peerDependencies": {
    "solid-js": "^1.5.0"
  }
}
